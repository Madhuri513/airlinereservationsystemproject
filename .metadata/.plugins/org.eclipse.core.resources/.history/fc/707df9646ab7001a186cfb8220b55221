package com.jfsfeb.airlinereservationsystem.dto;

import java.io.Serializable;

import lombok.Data;

@Data
@SuppressWarnings("serial")
public class UserDetails implements Serializable {
	
	private int userId;
	private String name;
	private String mailId;
	private String password;
	private long phoneNumber;
	
	public UserDetails() {
		
	}
		
	public UserDetails(int userId, String name, String mailId, String password, long phoneNumber) {
		this.userId = userId;
		this.name = name;
		this.mailId = mailId;
		this.password = password;
		this.phoneNumber = phoneNumber;
	}

	@Override
	public String toString() {
		return "userId=" + userId + ", name=" + name + ", mailId=" + mailId + ", phoneNumber="
				+ phoneNumber ;
	}	

}
